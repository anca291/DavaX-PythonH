{
  "title": "FastAPI Prometheus Dashboard",
  "panels": [
    {
      "type": "timeseries",
      "title": "Total Requests",
      "targets": [
        {
          "expr": "sum by (path) (http_requests_total)",
          "legendFormat": "{{path}}",
          "refId": "A"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "timeseries",
      "title": "Request Duration (avg in seconds)",
      "targets": [
        {
          "expr": "rate(http_request_duration_seconds_sum[1m]) / rate(http_request_duration_seconds_count[1m])",
          "legendFormat": "avg duration",
          "refId": "B"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "bargauge",
      "title": "Requests by Status Code",
      "targets": [
        {
          "expr": "sum by (status) (http_requests_total)",
          "legendFormat": "{{status}}",
          "refId": "C"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "stat",
      "title": "Total POST Calls",
      "targets": [
        {
          "expr": "sum(http_requests_total{method=\"POST\"})",
          "refId": "D"
        }
      ],
      "datasource": "Prometheus"
    }
  ],
  "editable": true,
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timezone": "browser",
  "schemaVersion": 34,
  "version": 1
}
